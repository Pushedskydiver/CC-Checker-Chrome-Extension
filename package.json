{
  "name": "cc-checker-chrome-extension",
  "version": "1.0.0",
  "description": "WCAG accessibility colour contrast checker chrome extension",
  "license": "ISC",
  "author": {
    "name": "Alex Clapperton",
    "email": "hi@alexclapperton.co.uk",
    "url": "https://alexclapperton.co.uk"
  },
  "dependencies": {
    "chroma-js": "2.0.3",
    "lodash.round": "^4.0.4",
    "lodash.throttle": "^4.1.1",
    "react": "16.10.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "16.10.0",
    "react-scripts": "4.0.0",
    "styled-components": "4.3.2",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.1",
    "@types/chrome": "0.0.126",
    "@types/lodash.round": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.9",
    "@types/styled-components": "^5.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "indent": [
            1,
            2
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
